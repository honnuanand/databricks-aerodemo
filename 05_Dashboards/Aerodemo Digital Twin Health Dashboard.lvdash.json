    "name" : "bdf6501b",
    "displayName" : "Digital Twin Engine Dataset",
    "query" : "SELECT\n  aircraft_id,\n  timestamp,\n  engine_temp,\n  vibration,\n  engine_rpm,\n  engine_health_status\nFROM arao.aerodemo.digital_twin_engine_view\nWHERE (\n  :aircraft_id IS NULL OR\n  :aircraft_id = 'NULL' OR\n  aircraft_id = :aircraft_id\n)",
    "parameters" : [ {
      "displayName" : "aircraft_id",
      "keyword" : "aircraft_id",
      "dataType" : "STRING",
      "defaultSelection" : {
        "values" : {
          "dataType" : "STRING",
          "values" : [ {
            "value" : "NULL"
          } ]
        }
      }
    } ]
  }, {
    "name" : "a08b619a",
    "displayName" : "Alerts Per Day",
    "query" : "SELECT \n  aircraft_id,\n  alert_day,\n  COUNT(*) AS alert_count\nFROM arao.aerodemo.anomaly_alerts\nWHERE \n  (:aircraft_id IS NULL OR :aircraft_id = 'NULL' OR aircraft_id = :aircraft_id)\n  AND (:start_date IS NULL OR alert_day >= :start_date)\n  AND (:end_date IS NULL OR alert_day <= :end_date)\nGROUP BY aircraft_id, alert_day\nORDER BY alert_day ASC",
    "parameters" : [ {
      "displayName" : "aircraft_id",
      "keyword" : "aircraft_id",
      "dataType" : "STRING",
      "defaultSelection" : {
        "values" : {
          "dataType" : "STRING",
          "values" : [ {
            "value" : "NULL"
          } ]
        }
      }
    }, {
