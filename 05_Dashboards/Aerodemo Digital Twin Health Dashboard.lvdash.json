    "name" : "572c1430",
    "displayName" : "Aircraft IDs",
    "query" : "SELECT DISTINCT aircraft_id\nFROM arao.aerodemo.digital_twin_aircraft_view\nORDER BY aircraft_id"
  }, {
    "name" : "f7cb50f5",
    "displayName" : "Engine Health Distribution",
    "query" : "SELECT \n  engine_health_status AS category, \n  COUNT(*) AS count\nFROM arao.aerodemo.digital_twin_engine_view\nWHERE (:aircraft_id IS NULL OR :aircraft_id = 'NULL' OR aircraft_id = :aircraft_id)\nGROUP BY engine_health_status\nORDER BY category",
    "parameters" : [ {
      "displayName" : "aircraft_id",
      "keyword" : "aircraft_id",
      "dataType" : "STRING",
      "defaultSelection" : {
        "values" : {
          "dataType" : "STRING",
          "values" : [ {
            "value" : "NULL"
          } ]
        }
      }
    } ]
  }, {
    "name" : "76a9342b",
    "displayName" : "Aircraft Temperature Distribution",
    "query" : "SELECT\n  CONCAT(FLOOR(engine_temp / 10) * 10, '-', FLOOR(engine_temp / 10) * 10 + 10) AS temp_bin,\n  COUNT(*) AS count\nFROM arao.aerodemo.digital_twin_engine_view\nWHERE (\n  :aircraft_id IS NULL OR\n  :aircraft_id = 'NULL' OR\n  aircraft_id = :aircraft_id\n)\nGROUP BY temp_bin\nORDER BY CAST(SPLIT(temp_bin, '-')[0] AS INT)",
    "parameters" : [ {
      "displayName" : "aircraft_id",
      "keyword" : "aircraft_id",
      "dataType" : "STRING",
      "defaultSelection" : {
        "values" : {
          "dataType" : "STRING",
          "values" : [ {
            "value" : "NULL"
          } ]
        }
      }
    } ]
  }, {
    "name" : "8a4e712b",
    "displayName" : "Aircraft Status Map View",
    "query" : "SELECT \n  aircraft_id AS `Aircraft`,\n  engine_health_status AS `Engine Health`,\n  alert_count AS `# Alerts`,\n  latitude,\n  longitude\nFROM arao.aerodemo.aircraft_status_map_view_v2"
  }, {
    "name" : "1c90e68b",
    "displayName" : "Component Engine Health",
    "query" : "SELECT\n  aircraft_id,\n  component_id,\n  thrust_level,\n  temperature_reading,\n  vibration_level,\n  oil_pressure,\n  fuel_consumption_rate,\n  health_status\nFROM arao.aerodemo.component_health_engine"
  }, {
    "name" : "65c09b65",
    "displayName" : "Engine Component Health Distribution",
    "query" : "SELECT health_status, COUNT(*) AS count\nFROM arao.aerodemo.component_health_engine\nGROUP BY health_status"
  }, {
    "name" : "928f97e8",
    "displayName" : "Fleet-Wide Health Status Distribution",
    "query" : "SELECT\n  health_status,\n  COUNT(*) AS count\nFROM arao.aerodemo.digital_twin_component_view\nGROUP BY health_status"
  }, {
    "name" : "e2b68858",
    "displayName" : "Component Health Breakdown by Type",
    "query" : "SELECT\n  component_type,\n  health_status,\n  COUNT(*) AS count\nFROM arao.aerodemo.digital_twin_component_view\nGROUP BY component_type, health_status"
  }, {
    "name" : "950c1366",
    "displayName" : "Heatmap â€“ Per Aircraft vs Health Status",
    "query" : "SELECT\n  aircraft_id,\n  health_status,\n  COUNT(*) as count\nFROM arao.aerodemo.digital_twin_component_view\nGROUP BY aircraft_id, health_status"
  }, {
    "name" : "0a9efb55",
    "displayName" : "Health Status Over Time",
    "query" : "SELECT \n  to_date(alert_timestamp) AS alert_date,\n  aircraft_id,\n  health_status,\n  COUNT(*) AS count\nFROM arao.aerodemo.anomaly_alerts_component\nGROUP BY alert_date, aircraft_id, health_status\nORDER BY alert_date, aircraft_id, health_status"
  }, {
    "name" : "0f92de12",
    "displayName" : "Component Time Alerts Trend",
    "query" : "SELECT \n  to_date(alert_timestamp) AS alert_day,\n  aircraft_id,\n  component_type,\n  health_status,\n  COUNT(*) AS alert_count\nFROM arao.aerodemo.anomaly_alerts_component\nGROUP BY alert_day, aircraft_id, component_type, health_status\nORDER BY alert_day, aircraft_id, component_type, health_status;"
  }, {
    "name" : "70cf3500",
    "displayName" : "Component Health Summary Table",
    "query" : "SELECT \n  aircraft_id,\n  component_type,\n  health_status,\n  COUNT(*) AS record_count\nFROM arao.aerodemo.digital_twin_component_view\nGROUP BY aircraft_id, component_type, health_status\nORDER BY aircraft_id, component_type, health_status;"
  }, {
    "name" : "3c294663",
    "displayName" : "Component Breakdown",
    "query" : "SELECT \n  aircraft_id,\n  component_type,\n  health_status,\n  COUNT(*) AS alert_count\nFROM arao.aerodemo.anomaly_alerts_component\nGROUP BY aircraft_id, component_type, health_status\nORDER BY aircraft_id, component_type, health_status;"
  }, {
    "name" : "d659180e",
    "displayName" : "Real-Time Component-Level Risk Distribution",
    "query" : "SELECT \n    airport.latitude,\n    airport.longitude,\n    comp.component_type,\n    comp.health_status,\n    COUNT(*) AS alert_count\nFROM arao.aerodemo.anomaly_alerts_component AS comp\nJOIN arao.aerodemo.aircraft_location_enriched_v2 AS airport\n    ON comp.aircraft_id = airport.aircraft_id\nWHERE comp.health_status IN ('CRITICAL', 'WARNING')\nGROUP BY airport.latitude, airport.longitude, comp.component_type, comp.health_status"
  } ],
