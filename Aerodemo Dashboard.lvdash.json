  }, {
    "name" : "f7cb50f5",
    "displayName" : "Engine Health Distribution",
    "query" : "SELECT \n  engine_health_status AS category, \n  COUNT(*) AS count\nFROM arao.aerodemo.digital_twin_engine_view\nWHERE (:aircraft_id IS NULL OR :aircraft_id = 'NULL' OR aircraft_id = :aircraft_id)\nGROUP BY engine_health_status\nORDER BY category",
    "parameters" : [ {
      "displayName" : "aircraft_id",
      "keyword" : "aircraft_id",
      "dataType" : "STRING",
      "defaultSelection" : {
        "values" : {
